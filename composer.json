{
    "name": "saade/filament-extra",
    "description": "A set of reusable Filament helpers, columns, fields, actions and more!",
    "keywords": [
        "saade",
        "laravel",
        "filament-extra"
    ],
    "homepage": "https://github.com/saade/filament-extra",
    "support": {
        "issues": "https://github.com/saade/filament-extra/issues",
        "source": "https://github.com/saade/filament-extra"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Saade",
            "email": "saade@outlook.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Saade\\FilamentExtra\\": "src/",
            "Saade\\FilamentExtra\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saade\\FilamentExtra\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saade\\FilamentExtra\\FilamentExtraServiceProvider"
            ],
            "aliases": {
                "FilamentExtra": "Saade\\FilamentExtra\\Facades\\FilamentExtra"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}